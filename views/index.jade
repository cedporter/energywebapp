extends layout

block content
  h1= title
  ul
    #container.col
        each result, i in results
          p #{i+1}: #{result.deviceName}
            li #{result.bulbType}
            li #{result.bulbWattage}
            li #{result.lastOnTime}
            li #{result.lastOffTime}
            li #{result.dailyDuration}
            li #{result.monthlyDuration}







